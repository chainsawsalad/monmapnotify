// var sprites1 = 'https://raw.githubusercontent.com/devsrc/pogo-sprites/master/sprites/clean/';
var sprites2 = 'https://raw.githubusercontent.com/PokeAPI/sprites/master/sprites/pokemon/';

for(var number = 1; number <= 720; number++) {
    localStorage.setItem('id' + number, sprites2 + number + '.png');
}
